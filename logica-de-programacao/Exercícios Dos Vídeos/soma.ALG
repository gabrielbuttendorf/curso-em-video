algoritmo "soma"
var
   n1, n2, soma: inteiro
inicio
      escreva("Digite um número: ")
      leia(n1)
      escreva("Digite mais um número: ")
      leia(n2)
      soma <- n1 + n2
      escreval("A soma entre esses números é: ",soma)
      //ou
      escreval("A soma entre",n1, " e",n2, " é:",soma)
fimalgoritmo