Algoritmo "Exercicio_66"

Var
   multiplicador, numero: inteiro
Inicio
   escreval("==============")
   escreval("   TABUADA")
   escreval("==============")
   escreval("")
   escreva("Digite um número para ver a tabuada: ")
   leia(numero)
   
   para multiplicador DE 0 ATE 10 PASSO 1 faca
      escreval(numero, " x", multiplicador, " =", (numero*multiplicador))
   fimPara
Fimalgoritmo
