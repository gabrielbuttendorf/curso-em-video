Algoritmo "Exercicio_46"

Var
   contador, soma: inteiro
Inicio
   contador <- 6
   enquanto (contador <= 100) faca
      escreva(contador)
      soma <- soma + contador
      contador <- contador + 2
   fimEnquanto
   escreval("")
   escreval("")
   escreval(" A soma de todos esses números é", soma)
Fimalgoritmo
