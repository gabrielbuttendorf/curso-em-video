Algoritmo "Exercicio_42"

Var
   numero, contador: inteiro
Inicio
   escreva("Digite um número inteiro positivo: ")
   leia(numero)

   escreval("")
   contador <- 1
   enquanto (contador <= numero) faca
      escreva(contador)
      contador <- contador + 1
   fimEnquanto
   
   escreva(" Acabou!")
   escreval("")
Fimalgoritmo
