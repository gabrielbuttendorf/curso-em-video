Algoritmo "Exercicio_11"

Var
   delta, B, A, C: real
Inicio
   escreval("Hoje vamos te ajudar a achar o valor de Delta!")
   escreva("Digite o valor de B: ")
   leia(B)
   escreva("Digite o valor de A: ")
   leia(A)
   escreva("Digite o valor de C: ")
   leia(C)
   delta <- (B^2) - (4*A*C)
   escreval("O valor de Delta é", delta)
Fimalgoritmo